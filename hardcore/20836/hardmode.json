import bsSpaz

SPAZ_BOT_INIT = bsSpaz.SpazBot.__init__

def __init__(self):
    self.defaultBombCount += 1
    self.curseTime += 5000
    self.run = True
    SPAZ_BOT_INIT(self)
    self.hitPoints = self.hitPointsMax = getattr(self, 'bot_hitpoints', 1000)
    self._punchPowerScale = getattr(self, 'bot_punch_powerscale', 2.0)
    self._impactScale = getattr(self, 'damage_cut', 0.5)

bsSpaz.SpazBot.__init__ = __init__